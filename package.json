    "react-scripts": "^5.0.1",
    "rimraf": "^5.0.1",
    "style-loader": "^3.3.2",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  },
  "browserslist": {
